{
	"name": "svelte-5-ai-unc.com",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20 <21"
	},
	"scripts": {
		"dev": "vite dev",
		"types": "supabase gen types --lang=typescript --linked --schema public > types/Database/supabase.ts",
		"types:local": "supabase gen types --lang=typescript --local --schema public > types/Database/supabase.ts",
		"update-db": "supabase db pull && supabase db reset && pnpm types:local",
		"lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-embed --plugin=prettier-plugin-sql --check .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-embed --plugin=prettier-plugin-sql --write .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"test": "pnpm test:unit && pnpm test:integration",
		"test:unit": "vitest",
		"test:integration": "playwright test",
		"build": "vite build",
		"preview": "vite preview",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.83.0",
		"@stripe/stripe-js": "^4.1.0",
		"@supabase/ssr": "^0.4.0",
		"@supabase/supabase-js": "^2.44.4",
		"@sveltejs/adapter-vercel": "^5.4.1",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@threlte/core": "8.0.0-next.15",
		"@threlte/extras": "9.0.0-next.22",
		"@types/nodemailer": "^6.4.15",
		"@types/strftime": "^0.9.8",
		"@types/three": "^0.167.1",
		"@vercel/analytics": "^1.3.1",
		"autoprefixer": "^10.4.19",
		"html5-qrcode": "^2.3.8",
		"lucide-svelte": "^0.416.0",
		"nodemailer": "^6.9.14",
		"postcss": "^8.4.40",
		"postcss-import": "^16.1.0",
		"prettier": "^3.3.3",
		"prettier-plugin-embed": "^0.4.15",
		"prettier-plugin-sql": "^0.18.1",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"qr-code-styling": "1.6.0-rc.1",
		"strftime": "^0.10.3",
		"stripe": "^16.5.0",
		"styled-qr-code-node": "^1.5.4",
		"svelte": "5.0.0-next.164",
		"svelte-check": "^3.8.4",
		"sveltekit-superforms": "^2.16.1",
		"tailwindcss": "^3.4.7",
		"three": "^0.167.1",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"vite": "^5.3.5",
		"zod": "^3.23.8"
	}
}
